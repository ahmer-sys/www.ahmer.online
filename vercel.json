{
  "version": 2,
  "name": "ahmer-portfolio",
  "public": true,
  "buildCommand": null,
  "outputDirectory": ".",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true,
    "enabled": true
  }
} 